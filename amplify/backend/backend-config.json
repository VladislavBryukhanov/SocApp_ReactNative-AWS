{
	"storage": {
		"userList": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		},
		"localDB": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"dbConnection": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "userList",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"createUser": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "userList",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"listUsers": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "userList",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"validateNewUser": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "SocAppMobile",
					"attributes": [
						"UserPoolId"
					]
				}
			]
		},
		"userPoolCleaner": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "SocAppMobile",
					"attributes": [
						"UserPoolId"
					]
				}
			]
		},
		"fetchProfile": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "userList",
					"attributes": [
						"Name",
						"Arn"
					]
				},
				{
					"category": "auth",
					"resourceName": "SocAppMobile",
					"attributes": [
						"UserPoolId"
					]
				}
			]
		}
	},
	"api": {
		"socAppApi": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "dbConnection",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"auth": {
		"SocAppMobile": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	}
}